#!/bin/sh


pick-emoji() {

  MENU=$(fzf << EOF
  dh
  area
  health
  finance
  career
  character
  home
  gb
  goals
  milestones
  projects
  achievements
  quests
  checkpoints
  EOF)

  case "$MENU" in
      dh) echo "ðŸ’¯" | pbcopy;;
      area) echo "î‰• " | pbcopy;;
      health) echo "ïŸ‘ " | copy;;
      finance) echo "ï˜– " | pbcopy;;
      career) echo "îž• " | pbcopy;;
      character) echo "ïš‹ " | pbcopy;;
      home) echo "ï€• " | pbcopy;;
      gb) echo "ïš«" | pbcopy;;
      goals) echo "ðŸŽ¯"| pbcopy;;
      milestones) echo "ðŸ†" | pbcopy;;
      achievements) echo "ï‚‘ " | pbcopy;;
      projects) echo "ï˜¬ " | pbcopy;;
      quests) echo "ç†" | pbcopy;;
      checkpoints) echo "è¼»" | pbcopy;;
  esac
}
